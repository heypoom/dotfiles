{
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "tcsh_history": {
        "title": "TCSH History",
        "description": "The tcsh history file format.",
        "convert-to-local-time": true,
        "regex": {
            "std": {
                "pattern": "^#(?<timestamp>\\+\\d+)\\n?(?<body>.*)?$"
            }
        },
        "sample": [
            {
                "line": "#+1375138067\necho HELLO=BAR"
            }
        ]
    }
}