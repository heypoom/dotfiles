declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/poom/Library/Application Support/JetBrains/Toolbox/apps/JetBrainsGateway/ch-0/213.5744.213/JetBrains Gateway.app/Contents/MacOS/gateway" $wait --args "${ideargs[@]}"