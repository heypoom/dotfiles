#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10774 at 2021-12-03T16:17:56.733535

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/poom/Library/Application Support/JetBrains/Toolbox/apps/IDEA-U/ch-0/213.5744.223/IntelliJ IDEA 2021.3 EAP.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"