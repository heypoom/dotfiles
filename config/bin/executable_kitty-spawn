#!/usr/bin/env bash

if ls /tmp/kitty* 1> /dev/null 2>&1
then
	echo Kitty is running.

  dst=$(printf '%s%s' unix: "$(echo /tmp/kitty* | awk '{print $1}')")
	echo "$dst"

	# Spawn a new Kitty window.
	/Applications/kitty.app/Contents/MacOS/kitty @ --to "$dst" launch --type os-window

	exit 0
fi

echo Kitty is *NOT* running.

cd "$HOME" || return

# Start Kitty.
# open "/Applications/kitty.app"
/Applications/kitty.app/Contents/MacOS/kitty fish

